
{{!-- Displays page with scraped data --}}

 <!-- Modal -->
	<div class="modal fade" id="myModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
	  <div class="modal-dialog" role="document">
	    <div class="modal-content">
	      <div class="modal-header">
	        <h5 class="modal-title" id="exampleModalLabel">Scraping...</h5>
	        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
	          <span aria-hidden="true">&times;</span>
	        </button>
	      </div>
	      <div class="modal-body">
	        <p>Articles Added!</p>
	      </div>
	      <div class="modal-footer">
	        <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
	      </div>
	    </div>
	  </div>
	</div>


<div class="card">
	{{#each title}}
  <h3 class="card-header">{{this.title}}</h3>
  <div class="card-block">
    <p class="card-text">{{this.id}}</p>
    <button type="button" class="btn btn-primary" id="savebutton">Save Article</button>
    {{/each}}
  </div>
</div>
